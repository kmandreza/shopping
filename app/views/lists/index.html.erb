<h1>Listing lists</h1>

<table>
  <tr>
    <th>Item</th>
    <th>Descrption</th>
    <th>Qty</th>
    <th>Store</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lists.each do |list| %>
  <tr>
    <td><%= list.item %></td>
    <td><%= list.descrption %></td>
    <td><%= list.qty %></td>
    <td><%= list.store %></td>
    <td><%= link_to 'Show', list %></td>
    <td><%= link_to 'Edit', edit_list_path(list) %></td>
    <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New List', new_list_path %>
